package com.smeportal.profileservice.response;

import lombok.Data;

@Data
public class ApiResponse<T> {
    private String status;
    private T data; //
    private String message;

    public ApiResponse(String status, T data, String messsage) {
        this.status = status;
        this.data = data;
        this.message = messsage;
    }

}
